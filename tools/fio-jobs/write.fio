[global]
ioengine=sync
direct=0
fadvise_hint=0
verify=0
rw=write
bs=1M
iodepth=64
invalidate=1
time_based=0
file_append=0
create_on_open=1
nrfiles=20
thread=1
openfiles=1
group_reporting=1
allrandrepeat=1
filename_format=$jobname.$jobnum.$filenum

[1_thread]
stonewall
directory=/mnt/disks/pd/600mb
filesize=600M
numjobs=1

